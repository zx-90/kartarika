
стат метод Запустить():
	// Положительные числа
	Консоль.Вывод(3 влево 1)
	Консоль.Вывод("\н")

	Консоль.Вывод(Целое8(3) влево Целое8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(3) влево Целое16(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое32(3) влево Целое32(4))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое64(3) влево Целое64(5))
	Консоль.Вывод("\н")

	// Отрицательные числа
	Консоль.Вывод(-1 влево 1)
	Консоль.Вывод("\н")

	Консоль.Вывод(Целое8(-1) влево Целое8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(-1) влево Целое16(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое32(-1) влево Целое32(4))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое64(-1) влево Целое64(5))
	Консоль.Вывод("\н")

	// Беззнаковые числа
	Консоль.Вывод(0ш1 влево 0ш1)
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(1) влево Счётное8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(1) влево Счётное16(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное32(1) влево Счётное32(4))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное64(1) влево Счётное64(5))
	Консоль.Вывод("\н")

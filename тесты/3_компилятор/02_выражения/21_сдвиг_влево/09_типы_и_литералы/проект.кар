
стат метод Запустить():
	Консоль.Вывод(Целое8(3) влево 1)
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(3) влево 0ш2)
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(3) влево 3)
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(3) влево 0ш4)
	Консоль.Вывод("\н")

	Консоль.Вывод(5 влево Целое8(1))
	Консоль.Вывод("\н")
	Консоль.Вывод(5 влево Целое16(2))
	Консоль.Вывод("\н")

	Консоль.Вывод(5 влево Счётное8(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(5 влево Счётное16(4))


стат метод Запустить():
	Консоль.Вывод(12 били 6)
	Консоль.Вывод("\н")
	Консоль.Вывод(0ш0В били 0ш6)
	Консоль.Вывод("\н")

	Консоль.Вывод(Целое8(-64) били Целое8(96))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(12) били Целое16(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое32(12) били Целое32(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое64(12) били Целое64(6))
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(12) били Счётное8(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(12) били Счётное16(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное32(12) били Счётное32(6))
	Консоль.Вывод("\н")

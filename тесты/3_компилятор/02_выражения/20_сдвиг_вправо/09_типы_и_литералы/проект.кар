
стат метод Запустить():
	Консоль.Вывод(Целое8(9) вправо 3)
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(9) вправо 0ш3)
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(9) вправо 3)
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(9) вправо 0ш3)
	Консоль.Вывод("\н")

	Консоль.Вывод(15 вправо Целое8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(15 вправо Целое16(2))
	Консоль.Вывод("\н")

	Консоль.Вывод(15 вправо Счётное8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(15 вправо Счётное16(2))
	Консоль.Вывод("\н")

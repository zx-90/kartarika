
стат метод Запустить():
	// Положительные числа
	Консоль.Вывод(127 вправо 1)
	Консоль.Вывод("\н")

	Консоль.Вывод(Целое8(127) вправо Целое8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(127) вправо Целое16(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое32(127) вправо Целое32(4))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое64(127) вправо Целое64(5))
	Консоль.Вывод("\н")

	// Отрицательные числа
	Консоль.Вывод(-128 вправо 1)
	Консоль.Вывод("\н")

	Консоль.Вывод(Целое8(-128) вправо Целое8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(-128) вправо Целое16(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое32(-128) вправо Целое32(4))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое64(-128) вправо Целое64(5))
	Консоль.Вывод("\н")

	// Беззнаковые числа
	Консоль.Вывод(0ш7Е вправо 0ш1)
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(127) вправо Счётное8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(127) вправо Счётное16(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное32(127) вправо Счётное32(4))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное64(127) вправо Счётное64(5))
	Консоль.Вывод("\н")

	// Беззнаковые числа с 1 в старшем бите
	Консоль.Вывод(0шЕЕЕЕЕЕЕЕ вправо 0ш1А)
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(255) вправо Счётное8(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(65535) вправо Счётное16(12))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное32(4294967295) вправо Счётное32(29))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное64(18446744073709551615) вправо Счётное64(62))
	Консоль.Вывод("\н")

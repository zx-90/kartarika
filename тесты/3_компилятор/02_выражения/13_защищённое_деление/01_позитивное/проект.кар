
стат метод Запустить():
	Консоль.Вывод(7 /! 2)
	Консоль.Вывод("\н")
	Консоль.Вывод(0ш09 /! 0ш2)
	Консоль.Вывод("\н")

	Консоль.Вывод(Целое8(7) /! Целое8(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(9) /! Целое16(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое32(11) /! Целое32(2))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое64(13) /! Целое64(2))
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(13) /! Счётное8(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(16) /! Счётное16(3))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное32(19) /! Счётное32(3))


стат метод Запустить():
	Консоль.Вывод(12 би 6)
	Консоль.Вывод("\н")
	Консоль.Вывод(0ш0В би 0ш6)
	Консоль.Вывод("\н")

	Консоль.Вывод(Целое8(-64) би Целое8(112))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(12) би Целое16(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое32(12) би Целое32(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое64(12) би Целое64(6))
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(12) би Счётное8(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(12) би Счётное16(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное32(12) би Счётное32(6))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное64(12) би Счётное64(6))

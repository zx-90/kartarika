
стат метод Запустить():
	Консоль.Вывод(Буль("Да") ! Нет)
	Консоль.Вывод("\н")

	Консоль.Вывод(Целое8("8") ! Целое8(0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Целое16("16") ! Целое16(0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Целое32("32") ! Целое32(0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Целое64("64") ! Целое64(0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Счётное8("8") ! Счётное8(0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Счётное16("16") ! Счётное16(0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Счётное32("32") ! Счётное32(0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Счётное64("64") ! Счётное64(0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Дробное32("32"))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Дробное64("64"))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Строка("У попа была собака"))
	Консоль.Вывод("\н")

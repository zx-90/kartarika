
стат метод Запустить():
	Консоль.Вывод(Дробное32("∞") ! Дробное32(0.0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Дробное32("+∞") ! Дробное32(0.0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Дробное32("Бесконечность") ! Дробное32(0.0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Дробное32("ПлюсБесконечность") ! Дробное32(0.0))
	Консоль.Вывод("\н")

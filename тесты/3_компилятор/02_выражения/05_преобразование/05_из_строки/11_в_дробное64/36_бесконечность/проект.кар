
стат метод Запустить():
	Консоль.Вывод(Дробное64("∞") ! Дробное64(0.0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Дробное64("+∞") ! Дробное64(0.0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Дробное64("Бесконечность") ! Дробное64(0.0))
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Дробное64("ПлюсБесконечность") ! Дробное64(0.0))
	Консоль.Вывод("\н")

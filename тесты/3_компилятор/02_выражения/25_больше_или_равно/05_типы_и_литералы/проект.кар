
стат метод Запустить():
	Консоль.Вывод(Целое8(2) бир 2)
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(3) бир 0ш2)
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(3) бир 2)
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(2) бир 0ш2)
	Консоль.Вывод("\н")

	Консоль.Вывод(Дробное32(2.0) бир 2)
	Консоль.Вывод("\н")
	Консоль.Вывод(Дробное32(3.0) бир 0ш2)
	Консоль.Вывод("\н")
	Консоль.Вывод(Дробное64(2.0) бир 2.0)
	Консоль.Вывод("\н")

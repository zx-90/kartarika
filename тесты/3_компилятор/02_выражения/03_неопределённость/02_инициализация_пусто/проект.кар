
стат метод Запустить():
	Консоль.Вывод(Буль?(Пусто) ! Да)
	Консоль.Вывод("\н")
	Консоль.Вывод(Буль?(Пусто) ! Нет)
	Консоль.Вывод("\н")
	
	Консоль.Вывод(Целое8?(Пусто) ! Целое8(8))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16?(Пусто) ! Целое16(16))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое32?(Пусто) ! Целое32(32))
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое64?(Пусто) ! Целое64(64))
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8?(Пусто) ! Счётное8(8))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16?(Пусто) ! Счётное16(16))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное32?(Пусто) ! Счётное32(32))
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное64?(Пусто) ! Счётное64(64))
	Консоль.Вывод("\н")

	Консоль.Вывод(Дробное32?(Пусто) ! Дробное32(32.0))
	Консоль.Вывод("\н")
	Консоль.Вывод(Дробное64?(Пусто) ! Дробное64(64.0))
	Консоль.Вывод("\н")

	Консоль.Вывод(Строка?(Пусто) ! "Да, пусто")

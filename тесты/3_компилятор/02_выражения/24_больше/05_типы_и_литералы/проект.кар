
стат метод Запустить():
	Консоль.Вывод(Целое8(2) бш 2)
	Консоль.Вывод("\н")
	Консоль.Вывод(Целое16(3) бш 0ш2)
	Консоль.Вывод("\н")

	Консоль.Вывод(Счётное8(3) бш 2)
	Консоль.Вывод("\н")
	Консоль.Вывод(Счётное16(2) бш 0ш2)
	Консоль.Вывод("\н")

	Консоль.Вывод(Дробное32(2.0) бш 2)
	Консоль.Вывод("\н")
	Консоль.Вывод(Дробное32(3.0) бш 0ш2)
	Консоль.Вывод("\н")
	Консоль.Вывод(Дробное64(2.0) бш 2.0)
	Консоль.Вывод("\н")
